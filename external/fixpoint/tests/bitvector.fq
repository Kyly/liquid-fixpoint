
// http://rise4fun.com/Z3/cxLS

bind 0 pos25 : {VV : Bitvector_t 32 | [VV = #x02000000]}
bind 1 x     : {VV : Bitvector_t 32 | []}
bind 2 y     : {VV : Bitvector_t 32 | [VV = Bitvector_or([x; pos25])]}
bind 3 z     : {VV : Bitvector_t 32 | [VV = Bitvector_and([y;pos25])]}

// sel-is-uif

constraint:
  env [ 0; 1; 2; 3 ]
  grd true
  lhs {VV : int | []}
  rhs {VV : int | [not (z = pos25)] }
  id 1

